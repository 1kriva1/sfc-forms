{
    "name": "sfc-forms",
    "version": "1.0.0",
    "description": "Street Football Club sfc-forms",
    "author": "Andrii Krivoruk",
    "scripts": {
        "webpack-dev": "cross-env NODE_ENV=development webpack --mode development",
        "webpack-prod": "cross-env NODE_ENV=production webpack --mode production",
        "build:dev": "webpack-dev-server --inline --hot --progress --port 8080",
        "build:prod": "npm run build:clean && ngc && npm run webpack-prod && npm run build:clean",
        "build:clean": "del-cli index.js index.d.ts *.ngsummary.json *.metadata.json *.js.map src/**/*.js src/**/*.js.map src/**/*d.ts src/**/*.ngsummary.json src/**/*.metadata.json src/**/**/*.ngfactory.ts src/**/*.ngstyle.ts src/**/*.shim.ts",
        "serve": "lite-server",
        "test": "rimraf ./coverage && karma start"
    },
    "dependencies": {
        "@angular/common": "~8.0.0",
        "@angular/compiler": "~8.0.0",
        "@angular/compiler-cli": "~8.0.0",
        "@angular/core": "~8.0.0",
        "@angular/forms": "~8.0.0",
        "@angular/platform-browser": "~8.0.0",
        "@angular/platform-browser-dynamic": "~8.0.0",
        "rxjs": "~6.4",
        "core-js": "^2.5.7",
        "zone.js": "0.9.1"
    },
    "devDependencies": {
        "@types/node": "12.0.0",
        "@types/core-js": "^2.5.0",
        "@types/jasmine": "^2.8.8",
        "@ngtools/webpack": "~7.3",
        "typescript": "3.4.5",
        "webpack": "4.33.0",
        "webpack-cli": "3.3.0",
        "webpack-dev-server": "3.6.0",
        "webpack-merge": "~4.2",
        "awesome-typescript-loader": "~5.2",
        "angular2-template-loader": "~0.6",
        "angular-router-loader": "^0.8.5",
        "html-loader": "0.5.5",
        "style-loader": "~0.23",
        "css-loader": "2.1.0",
        "to-string-loader": "1.1.6",
        "uglifyjs-webpack-plugin": "~2.0",
        "optimize-css-assets-webpack-plugin": "~5.0",
        "clean-webpack-plugin": "~0.1",
        "html-webpack-plugin": "3.2.0",
        "cssnano": "~4.1",
        "cross-env": "~5.2",
        "del-cli": "~1.1",
        "lite-server": "~2.4",
        "concurrently": "3.2.0",
        "source-map-loader": "^0.2.4",
        "ts-loader": "^4.4.2",
        "raw-loader": "0.5.1",
        "istanbul-instrumenter-loader": "^3.0.1",
        "file-loader": "1.1.11",
        "jasmine": "^3.2.0",
        "jasmine-core": "^3.2.0",
        "karma": "^3.0.0",
        "karma-chrome-launcher": "^2.2.0",
        "karma-coverage": "^1.1.2",
        "karma-jasmine": "^1.1.2",
        "karma-jasmine-html-reporter": "^1.3.1",
        "karma-mocha-reporter": "^2.2.5",
        "karma-remap-coverage": "^0.1.5",
        "karma-sourcemap-loader": "^0.3.7",
        "karma-webpack": "^3.0.4"
    }
}
